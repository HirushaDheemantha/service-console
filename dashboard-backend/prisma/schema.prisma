datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Service {
  id            Int      @id @default(autoincrement())
  type          String
  name          String
  purchasePrice Float
  renewalPrice  Float
  purchaseDate  DateTime
  renewalDate   DateTime
  status        String
}

model Client {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  phone     String
  repoLink  String
  driveLink String
}
